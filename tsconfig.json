{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["./src/components/*"],
      "@layouts/*": ["./src/layouts/*"],
      "@services/*": ["./src/services/*"],
      "@utils/*": ["./src/utils/*"],
      "@styles/*": ["./src/styles/*"],
      "@views/*": ["./src/views/*"],
      "@router/*": ["./src/router/*"],
      "@solid/*": ["./src/solid/*"],
      "@jikan/*": ["./src/jikan/*"]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  }
}
